<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    package="com.tistory.comfy91.excuseme_android">    <!-- 인터넷 -->    <uses-permission android:name="android.permission.INTERNET" />    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <!-- 로컬 이미지 로드 -->    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    <!-- 음성 녹음 -->    <uses-permission android:name="android.permission.RECORD_AUDIO" />    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/AppTheme"        android:networkSecurityConfig="@xml/network_security_config"        android:windowSoftInputMode="adjustResize"        tools:ignore="GoogleAppIndexingWarning">        <activity android:name=".feature.detailcard.DetailCardActivity"        android:exported="true"/>        <activity android:name=".feature.modcard.ModCardActivity"></activity>        <activity            android:name=".feature.disabled.DisabledActivity"            android:exported="true">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity            android:name=".feature.splash.SplashActivity"            android:exported="true" />        <activity            android:name=".test.LottieActivity"            android:exported="true" />        <activity android:name=".feature.setting.SettingActivity"            android:exported="true" />        <activity            android:name=".feature.cg_password.ChangePasswordActivity"            android:exported="true"            android:screenOrientation="portrait"            android:windowSoftInputMode="adjustResize|adjustPan" />        <activity android:name=".feature.cg_phonenum.ChangePhoneNumActivity"                  android:exported="true" />        <activity            android:name=".feature.download_card.DownloadCardActivity"            android:exported="true" />        <activity            android:name=".feature.addcard.AddCardActivity"            android:exported="true"/>        <activity            android:name=".feature.unlock.UnlockActivity"            android:windowSoftInputMode="adjustResize"            android:exported="true"/>        <activity            android:name=".feature.helper_sort.HelperSortActivity"            android:exported="true" />        <activity            android:name=".feature.helper.HelperActivity"            android:exported="true" />    </application></manifest>