<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/container2"    android:layout_width="match_parent"    android:layout_height="match_parent">    <FrameLayout        android:id="@+id/frameHelper"        android:layout_width="match_parent"        android:layout_height="0dp"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintEnd_toEndOf="parent"/>        <View            android:id="@+id/backHelperBlur"            android:visibility="invisible"            android:background="#CCFFFFFF"            android:layout_width="match_parent"            android:layout_height="match_parent"/>        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="match_parent">            <LinearLayout                android:id="@+id/cstHelperBottom"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:background="@drawable/bg_sq_shadow"                android:orientation="horizontal"                android:outlineSpotShadowColor="@color/black_10"                android:visibility="visible"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent">                <LinearLayout                    android:id="@+id/btnHelperGoDisabled"                    android:layout_width="0dp"                    android:layout_height="wrap_content"                    android:layout_weight="1"                    android:gravity="center"                    android:orientation="vertical"                    app:layout_constraintTop_toTopOf="parent">                    <ImageView                        android:layout_marginTop="6dp"                        android:layout_width="30dp"                        android:layout_height="30dp"                        android:src="@drawable/btn_helpertab_helper" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:fontFamily="@font/notosans_bold"                        android:text="홈 미리보기"                        android:textAlignment="center"                        android:textColor="@color/greyish_two"                        android:textSize="10sp" />                </LinearLayout>                <LinearLayout                    android:id="@+id/btnHelperAllCard"                    android:layout_width="0dp"                    android:layout_height="wrap_content"                    android:layout_weight="1"                    android:gravity="center"                    android:orientation="vertical"                    app:layout_constraintTop_toTopOf="parent">                    <ImageView                        android:layout_marginTop="6dp"                        android:layout_width="30dp"                        android:layout_height="30dp"                        android:src="@drawable/btn_helpertab_helpersort" />                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:fontFamily="@font/notosans_bold"                        android:text="카드관리"                        android:textAlignment="center"                        android:textColor="@color/greyish_two"                        android:textSize="10sp" />                </LinearLayout>            </LinearLayout>            <LinearLayout                android:id="@+id/btnHelperNewCard"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginBottom="6dp"                android:clickable="true"                android:orientation="horizontal"                android:visibility="invisible"                app:layout_constraintBottom_toTopOf="@id/btnHelperAddCard"                app:layout_constraintStart_toStartOf="@id/btnHelperAddCard">                <com.google.android.material.floatingactionbutton.FloatingActionButton                    android:layout_width="58dp"                    android:layout_height="58dp"                    android:elevation="0dp"                    app:elevation="0dp"                    android:foreground="@drawable/btn_home_newcard" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:layout_marginStart="10dp"                    android:fontFamily="@font/notosans_medium"                    android:text="새로 만들기"                    android:textColor="@color/mainpink" />            </LinearLayout>            <LinearLayout                android:id="@+id/btnHelperDownCard"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginBottom="6dp"                android:clickable="true"                android:orientation="horizontal"                android:visibility="invisible"                app:layout_constraintBottom_toTopOf="@id/btnHelperNewCard"                app:layout_constraintStart_toStartOf="@id/btnHelperAddCard">                <com.google.android.material.floatingactionbutton.FloatingActionButton                    android:layout_width="58dp"                    android:layout_height="58dp"                    android:elevation="0dp"                    app:elevation="0dp"                    android:foreground="@drawable/btn_home_download" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:layout_marginStart="10dp"                    android:fontFamily="@font/notosans_medium"                    android:text="내려받기"                    android:textColor="@color/mainpink" />            </LinearLayout>            <com.google.android.material.floatingactionbutton.FloatingActionButton                android:id="@+id/btnHelperAddCard"                android:layout_width="58dp"                android:layout_height="58dp"                android:layout_marginBottom="21dp"                android:foreground="@drawable/btn_fab_add"                android:visibility="visible"                app:layout_constraintBottom_toBottomOf="parent"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintStart_toStartOf="parent" />        </androidx.constraintlayout.widget.ConstraintLayout></androidx.constraintlayout.widget.ConstraintLayout>